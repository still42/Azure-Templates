{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vnet-extern": {
            "type": "string",
            "defaultValue": "vnet-extern"
        },
        "snet-extern-attacker": {
            "type": "string",
            "defaultValue": "snet-extern-attacker"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [
        {
            "name": "[parameters('vnet-extern')]",
            "type": "Microsoft.Network/virtualNetworks",
            "apiVersion": "2019-11-01",
            "location": "[resourceGroup().location]",
            "properties": {
                "addressSpace": {
                    "addressPrefixes": [
                        "192.168.0.0/24"
                    ]
                },
                "dhcpOptions": {
                    "dnsServers": ["10.0.10.10"]
                },
                "subnets": [
                    {
                        "name": "[parameters('snet-extern-attacker')]",
                        "properties": {
                            "addressPrefix": "192.168.0.0/24"
                        }
                    }
                ]
            }
        }
    ],
    "outputs": {}
}
